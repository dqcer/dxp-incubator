<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 5.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-5.0.dtd">
<hibernate-mapping>
    <class name="com.dqcer.test.entity.MdsUser" table="mds_user">
        <id name="id">
            <column name="id" />
            <generator class="native" />
        </id>

                    <!--状态-->
                <property name="status">
                    <column name="status" />
                </property>
                    <!--创建人-->
                <property name="createdBy">
                    <column name="created_by" />
                </property>
                    <!--创建时间-->
                <property name="createdTime">
                    <column name="created_time" />
                </property>
                    <!--更新人-->
                <property name="updatedBy">
                    <column name="updated_by" />
                </property>
                    <!--更新时间-->
                <property name="updatedTime">
                    <column name="updated_time" />
                </property>
                    <!--平台统一账户编号 平台分配的-->
                <property name="uniqueAccountCode">
                    <column name="unique_account_code" />
                </property>
                    <!--类型 (1内部或2外部)-->
                <property name="type">
                    <column name="type" />
                </property>
                    <!--活动状态 活动状态-->
                <property name="ativce">
                    <column name="ativce" />
                </property>
                    <!--详细信息 人员详细信息表id-->
                <property name="detailinfoId">
                    <column name="detailInfo_id" />
                </property>
                    <!--工号 客户自定义-->
                <property name="customerCode">
                    <column name="customer_code" />
                </property>
                    <!--性别-->
                <property name="sex">
                    <column name="sex" />
                </property>
                    <!--名字-->
                <property name="lastName">
                    <column name="last_name" />
                </property>
                    <!--中间名-->
                <property name="midName">
                    <column name="mid_name" />
                </property>
                    <!--姓氏-->
                <property name="firstName">
                    <column name="first_name" />
                </property>
                    <!--职位 可选-->
                <property name="position">
                    <column name="position" />
                </property>
                    <!--组织 可选-->
                <property name="organizationId">
                    <column name="organization_id" />
                </property>
                    <!--语言-->
                <property name="language">
                    <column name="language" />
                </property>
                    <!--时区-->
                <property name="zone">
                    <column name="zone" />
                </property>
                    <!--邮编-->
                <property name="zipcode">
                    <column name="zipcode" />
                </property>
                    <!--头衔-->
                <property name="title">
                    <column name="title" />
                </property>
                    <!--是否为账号 1/是 2/否-->
                <property name="hasAccount">
                    <column name="has_account" />
                </property>
    </class>
</hibernate-mapping>
